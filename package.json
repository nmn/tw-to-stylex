{
  "name": "tailwind-to-stylex",
  "version": "0.1.3",
  "description": "A utility for converting Tailwind CSS to Stylex",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "tw-to-stylex": "./lib/tw-to-stylex.js"
  },
  "repository": "https://www.github.com/facebook/stylex",
