  },
  "jest": {},
  "files": [
    "lib/*"
  ],
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-flow": "^7.23.3",
    "babel-plugin-syntax-hermes-parser": "^0.18.0",
    "flow-api-translator": "^0.18.0",
    "flow-bin": "^0.226.0",
    "jest": "^30.0.0-alpha.1",
    "prettier": "^3.1.0",
    "yargs": "^17.7.2"
